<h2>
  <% if @marker_symbol %>
    Adult Expression Data for <%= @marker_symbol %> (<%= @colony_prefix %>)
  <% else %>
    Adult Expression Data for <%= @colony_prefix %>
  <% end %>
</h2>

<div id="test-description">
  <%= @test["description"] %>
</div>

<div id="test-parameter-groups">
  <div class="test-parameter-group">
    <% if @test["second_description"] %>
      <p><%= @test["second_description"] %></p>
    <% end %>
  </div>
  
  <div class="accordion">
    <h6><a href="">Ticklist</a></h6>
    <div>
      <%= partial( "datasets/sanger-phenotyping/adult_expression_ticklist" ) %>
    </div>

    <h6><a href="">Images</a></h6>
    <div id="sanger-phenotyping-adult-expression-images">
      <%= partial( "datasets/sanger-phenotyping/adult_expression_images" ) %>
    </div>
  </div>
</div>
