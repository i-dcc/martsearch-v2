      "pipeline",
      "europhenome_id",
      "line_name",
      "sex",
      "zygosity",
      "allele_id",
      "allele_name",
      "emma_id",
      "escell_clone",
      "stocklist_id",
      "parameter_eslim_id",
      "parameter_name",
      "significance",
      "effect_size",
      "mp_term",
      "mp_term_name"
    ],
    "filters": {}
  }
