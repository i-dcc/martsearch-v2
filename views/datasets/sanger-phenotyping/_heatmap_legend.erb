<table id="sanger-phenotyping_heatmap_legend" class="nobg">
  <tr>
    <th class="noborder">Legend:</th>
    <td class="completed_data_available pheno_result"></td>
    <td class="noborder">test complete and data/resources available</td>
    <td class="significant_difference pheno_result"></td>
    <td class="noborder">test considered interesting</td>
    <td class="early_indication_of_possible_phenotype pheno_result"></td>
    <td class="noborder">early indication of possible phenotype</td>
  </tr>
  <tr>
    <td class="noborder"></td>
    <td class="no_significant_difference pheno_result"></td>
    <td class="noborder">test done but not considered interesting</td>
    <td class="not_applicable pheno_result"></td>
    <td class="noborder">test not applicable, e.g. no LacZ, therefore expression not possible</td>
    <td class="test_not_done pheno_result"></td>
    <td class="noborder">test not carried out</td>
  </tr>
  <tr>
    <td class="noborder"></td>
    <td class="pheno_result noborder"><a href="#sanger-phenotyping_heatmap_legend"><img src="<%= BASE_URI %>/images/bullet_go.png" /></a></td>
    <td class="noborder">link to a phenotyping test report page</td>
    <td class="noborder"></td>
    <td class="noborder"></td>
    <td class="noborder"></td>
    <td class="noborder"></td>
  </tr>
</table>