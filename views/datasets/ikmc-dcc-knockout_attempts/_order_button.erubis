<% if project["pipeline_name"] === "KOMP-CSD" %>
  <% if order_type === "vector" %>
    <a href="http://www.komp.org/vectorOrder.php?projectid=<%= ikmc_project_id %>" target="_blank" class="order">order</a>
  <% elsif order_type === "clone" %>
    <a href="http://www.komp.org/orders.php?project=CSD<%= ikmc_project_id %>&amp;product=1" target="_blank" class="order">order</a>
  <% else %>
    <a href="http://www.komp.org/geneinfo.php?project=CSD<%= ikmc_project_id %>" target="_blank" class="order">order</a>
  <% end %>
<% elsif project["pipeline_name"] === "EUCOMM" %>
  <a href="http://www.eummcr.org/order.php" target="_blank" class="order">order</a>
<% end %>