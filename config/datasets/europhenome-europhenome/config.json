{
  "display":              true,
  "use_in_search":        true,
  "index":                true,
  "custom_sort":          true,
  "custom_css":           true,
  "custom_js":            true,
  "custom_view_helpers":  true,
  "custom_routes":        false,
  "url":                  "http://www.europhenome.org/biomart",
