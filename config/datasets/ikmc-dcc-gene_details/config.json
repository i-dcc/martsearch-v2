{
  "display":              true,
  "use_in_search":        true,
  "index":                true,
  "custom_sort":          true,
  "custom_css":           false,
  "custom_js":            true,
  "custom_view_helpers":  false,
  "custom_routes":        false,
  "url":                  "http://www.i-dcc.org/biomart",
  "display_name":         "Gene Details",
  "dataset_name":         "dcc",
  "attribution":          "The International Knockout Mouse Consortium (IKMC)",
  "attribution_link":     "http://www.knockoutmouse.org/",
  "description":          "",
  "indexing": {
    "allow_document_creation": true,
    "attribute_map": [
      { "attr": "mgi_accession_id", "idx": "mgi_accession_id_key", "use_to_map": true },
      { "attr": "marker_symbol",    "idx": "marker_symbol" },
      { "attr": "marker_name",      "idx": "marker_name" },
      { "attr": "chromosome",       "idx": "chromosome" },
      { "attr": "strand",           "idx": "strand" },
      { "attr": "start",            "idx": "coord_start" },
      { "attr": "end",              "idx": "coord_end" },
      { "attr": "synonym",          "idx": "synonym" },
      { "attr": "ensembl_gene_id",  "idx": "ensembl_gene_id" },
      { "attr": "vega_gene_id",     "idx": "vega_gene_id" },
      { "attr": "ncbi_gene_id",     "idx": "ncbi_gene_id" },
      { "attr": "ccds_id",          "idx": "ccds_id" },
      { "attr": "omim_id",          "idx": "omim_id" },
      { "attr": "omim_description", "idx": "omim_desc" },
      { "attr": "ikmc_project",     "idx": "ikmc_project" },
      { "attr": "ikmc_project_id",  "idx": "ikmc_project_id" },
      { "attr": "vector_generated", "idx": "product_status", "if_attr_equals": ["1"], "index_attr_name": true },
      { "attr": "vector_available", "idx": "product_status", "if_attr_equals": ["1"], "index_attr_name": true },
      { "attr": "escell_generated", "idx": "product_status", "if_attr_equals": ["1"], "index_attr_name": true },
      { "attr": "escell_available", "idx": "product_status", "if_attr_equals": ["1"], "index_attr_name": true },
      { "attr": "mouse_generated",  "idx": "product_status", "if_attr_equals": ["1"], "index_attr_name": true },
      { "attr": "mouse_available",  "idx": "product_status", "if_attr_equals": ["1"], "index_attr_name": true }
    ],
    "grouped_attributes": [
      { "attrs": ["ikmc_project","vector_generated"], "idx": "ikmc_project_product_status", "using": "_" },
      { "attrs": ["ikmc_project","vector_available"], "idx": "ikmc_project_product_status", "using": "_" },
      { "attrs": ["ikmc_project","escell_generated"], "idx": "ikmc_project_product_status", "using": "_" },
      { "attrs": ["ikmc_project","escell_available"], "idx": "ikmc_project_product_status", "using": "_" },
      { "attrs": ["ikmc_project","mouse_generated"],  "idx": "ikmc_project_product_status", "using": "_" },
      { "attrs": ["ikmc_project","mouse_available"],  "idx": "ikmc_project_product_status", "using": "_" }
    ]
  },
  "searching": {
    "joined_index_field":       "mgi_accession_id_key",
    "joined_biomart_filter":    "mgi_accession_id",
    "joined_biomart_attribute": "mgi_accession_id",
    "attributes": [
      "marker_symbol",
      "marker_name",
      "synonym",
      "chromosome",
      "start",
      "end",
      "strand",
      "mgi_accession_id",
      "ensembl_gene_id",
      "vega_gene_id",
      "ncbi_gene_id",
      "ccds_id",
      "omim_id",
      "omim_description"
    ],
    "filters": {}
  }
}