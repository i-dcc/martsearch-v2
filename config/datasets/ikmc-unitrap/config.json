{
  "display":              true,
  "use_in_search":        true,
  "index":                true,
  "custom_sort":          true,
  "custom_css":           false,
  "custom_js":            false,
  "custom_view_helpers":  false,
  "custom_routes":        false,
  "url":                  "http://htgt.internal.sanger.ac.uk:3010/biomart",
  "dataset_name":         "unitrap",
  "display_name":         "UniTrap Trapped Products",
  "attribution":          "UniTrap",
  "attribution_link":     "http://unitrap.cbm.fvg.it/",
  "description":          "Gene trapped mutant ES cells and vectors identified by the UniTrap resource.",
  "indexing": {
    "allow_document_creation": false,
    "attribute_map": [
      { "attr": "ensembl_gene_id",       "idx": "ensembl_gene_id",     "use_to_map": true },
      { "attr": "ensembl_transcript_id", "idx": "ensembl_transcript_id" },
      { "attr": "ensembl_exon_id",       "idx": "ensembl_exon_id" },
      { "attr": "unitrap_accession_id",  "idx": "unitrap_accession_id" },
      { "attr": "escell_clone",          "idx": "escell" }
    ]
  },
  "searching": {
    "joined_index_field":       "ensembl_gene_id",
    "joined_biomart_filter":    "ensembl_gene_id",
    "joined_biomart_attribute": "ensembl_gene_id",
    "attributes": [
      "ensembl_gene_id",
      "ensembl_transcript_id",
      "ensembl_exon_id",
      "unitrap_accession_id",
      "genbank_id",
      "genbank_accession",
      "ambiguous",
      "project",
      "trap_name",
      "vector_name",
      "escell_clone",
      "escell_line",
      "escell_strain",
      "design_type"
    ],
    "filters": {
      "pipeline": ["EGTC","EUCOMM","TIGM"]
    }
  }
}